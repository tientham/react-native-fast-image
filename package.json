{
    "name": "react-native-fast-image",
    "version": "8.6.3",
    "description": "ðŸš© FastImage, performant React Native image component.",
    "keywords": [
        "cache",
        "cached",
        "fastimage",
        "image",
        "priority"
    ],
    "homepage": "https://github.com/DylanVann/react-native-fast-image#readme",
    "bugs": {
        "url": "https://github.com/DylanVann/react-native-fast-image/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DylanVann/react-native-fast-image.git"
    },
    "license": "(MIT AND Apache-2.0)",
    "author": "Dylan Vann <dylan@dylanvann.com> (https://dylanvann.com)",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "android",
        "!android/build",
        "ios",
        "!ios/build",
        "dist",
        "RNFastImage.podspec"
    ],
    "scripts": {
        "bob": "bob build",
        "prepack": "bob build",
        "build": "dv-scripts build && cp src/index.js.flow dist/index.js.flow && cp src/index.js.flow dist/index.cjs.js.flow",
        "lint": "dv-scripts lint",
        "release": "dv-scripts release",
        "test": "dv-scripts test",
        "compack": "npm pack"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all"
    },
    "eslintConfig": {
        "extends": "dv-scripts"
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "ReactNativeFastImageExample*",
            "ReactNativeFastImageExampleServer*"
        ],
        "modulePathIgnorePatterns": [
            "ReactNativeFastImageExample*",
            "ReactNativeFastImageExampleServer*"
        ],
        "preset": "react-native"
    },
    "resolutions": {
        "@jest/create-cache-key-function": "^27"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/runtime": "^7.14.6",
        "@types/jest": "^26.0.24",
        "@types/react": "~17.0.21",
        "@types/react-native": "0.70.0",
        "@types/react-test-renderer": "^17.0.1",
        "dv-scripts": "^1.6.0",
        "eslint-config-dv-scripts": "^1.1.1",
        "metro-react-native-babel-preset": "^0.66.1",
        "prettier": "^2.3.2",
        "react": "18.2.0",
        "react-native": "0.72.5",
        "react-test-renderer": "18.2.0",
        "typescript": "^4.3.5"
    },
    "peerDependencies": {
        "react": "^17 || ^18",
        "react-native": ">=0.60.0"
    },
    "dependencies": {
        "bob": "^2.7.0",
        "react-native-builder-bob": "^0.23.1"
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
          "commonjs",
          "module",
          "typescript"
        ]
    }
}
